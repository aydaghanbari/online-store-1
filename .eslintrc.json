{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "plugin:react/recommended",
        "airbnb"
       // "prettier"
        //"prettier/react"
        

       
        

    ],
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": [
        "react"
       // "prettier"
    ],
    "rules": {
        "anchor-has-content" : 0,
        "prefer-statless-function" : 0,
        "linebreak-style" : 0,
        "react/jsx-filename-extension": 0,
        "anchor-is-valid" : 0
       
       
    }
}
